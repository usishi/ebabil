!!!5
html
	head
		meta(HTTP-EQUIV="Pragma",CONTENT="no-cache")
		meta(name="viewport",content="width=device-width, maximum-scale=1, initial-scale=1, user-scalable=0")
		meta(charset="utf-8")
		//cssdebug
		link(rel="stylesheet",href="/static/css/bootstrap.css",media="screen",rtype="text/css")
		link(rel="stylesheet",href="/static/css/bootstrap-responsive.css",media="screen",rtype="text/css")
		link(rel="stylesheet",href="/static/css/font-awesome.css",media="screen",rtype="text/css")
		link(rel="stylesheet",href="/static/css/ace.css",media="screen",rtype="text/css")
		link(rel="stylesheet",href="/static/css/ace-responsive.css",media="screen",rtype="text/css")
		//endcssdebug
		//if IE 7
			link(rel="stylesheet",href="/static/css/font-awesome-ie7.css",media="screen",rtype="text/css")
		//if lt IE 9
			link(rel="stylesheet",href="/static/css/ace-ie.css",media="screen",rtype="text/css")
		link(rel="stylesheet",href="/static/css/main.css",media="screen",rtype="text/css")		
		link(rel="stylesheet",href="/static/css/jquery-ui-1.10.3.custom.css",media="screen",rtype="text/css")		
		link(rel="stylesheet",href="/static/css/jquery.gritter.css",media="screen",rtype="text/css")		
		block styles
	body
		div#wrap
			div.navbar.navbar-inverse
				div.navbar-inner
					div.container-fluid(style="padding-right:0px;padding-left:40px;")
						div.pull-left
							a(href="/pg/dashboard")
								img(src="/static/img/buluthan_logo_mini.png")					
						ul.nav.ace-nav.pull-right
							li.grey
								a.dropdown-toggle(data-toggle="dropdown",href="#")
									i.icon-tasks
									span#jobicon
								ul.pull-right.dropdown-navbar.dropdown-menu.dropdown-caret.dropdown-closer#joblist
									
							li.purple
								a.dropdown-toggle(data-toggle="dropdown",href="#")
									i.icon-warning-sign.icon-only#logicon
									span.badge.badge-important
								ul.pull-right.dropdown-navbar.navbar-pink.dropdown-menu.dropdown-caret.dropdown-closer(style="width:400px;")#loglist
								
							//li.green.hide
								a.dropdown-toggle(data-toggle="dropdown",href="#")
									i.icon-comment-alt.icon-only.icon-animated-vertical
									| 5
								ul.pull-right.dropdown-navbar.navbar-green.dropdown-menu.dropdown-caret.dropdown-closer
									li.nav-header
										i.icon-comment
										| 5 not var
										a.btn.button.btn-mini(href="#",onClick="$('#modal_AddComment').modal();") 
											i.icon.icon-plus-sign 
											| ekle
									li
										a
											img.msg-photo(src="/static/avatars/avatar.png")
											span.msg-body
												span.msg-title
													span.blue 
														| Göktuğ :
													| Selam dostum, yeni makinelere dikkat
												span.msg-time
													i.icon-time
													span
														| 1 dakika önce
							li.light-blue.user-profile
								a.user-menu.dropdown-toggle(data-toggle="dropdown",href="#")
									//img.nav-user-photo(src="/static/avatars/user.jpg")
									span#user_info
										small
											| Hoşgeldin,
										| !{user.name}
									i.icon-carret-down
								ul.pull-right.dropdown-menu.dropdown-yellow.dropdown-caret.dropdown-closer#user-menu
									li
										a(href="#",onClick="logoff();")
											i.icon-off
											| Sistemden Çıkış Yap						
			div.container-fluid#main-container
				a#menu-toggler(href="#")
					span
				div#sidebar
					!{user.menu}
				div.clearfix#main-content
					div#breadcrumbs
						ul.breadcrumb
							block breadcrumb
					div.clearfix#page-content				
						div.page-header.positive-relative
							h1
								block pagetitle
						div.row-fluid
							block pagecontent
		div#push
	div#footer
		div(style="padding:5px;text-align:center")
			| Lisans Sahibi 
			img(src="data:image/png;base64,#{Config.customer.customerlogo}")	
			| &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
			| Geliştirici 
			img(src="data:image/png;base64,")

			div.modal.hide.fade(style="heigth:70%")#modal_AddComment
				div.modal-header
					h5 Yeni Not Ekleme
				div.modal-body			
					div.row-fluid
						div.span4
							| Notunuzu Giriniz :
						div.span6
							input(type="text")	

				div.modal-footer	
					a.btn.btn-small.btn-primary(href="#",onClick="$('#modal_AddComment').modal('hide');") Vazgeç	
					a.btn.btn-small.btn-primary(href="#",onClick="selectOU();") Kaydet					
		div#usr.hide
			!{user.encoded}
	//jsdebug
	script(src="/static/js/3rdParty/jquery-1.9.1.js",type="text/javascript")
	script(src="/static/js/3rdParty/bootstrap.js",type="text/javascript")	
	script(src="/static/js/3rdParty/jquery-ui-1.10.3.custom.js",type="text/javascript")
	script(src="/static/js/3rdParty/jquery.ui.touch-punch.js",type="text/javascript")
	script(src="/static/js/3rdParty/jquery.slimscroll.js",type="text/javascript")
	script(src="/static/js/3rdParty/jquery.gritter.js",type="text/javascript")
	script(src="/static/js/3rdParty/jq-blockui.js",type="text/javascript")
	script(src="/static/js/3rdParty/ace-elements.js",type="text/javascript")
	script(src="/static/js/3rdParty/ace.js",type="text/javascript")
	script(src="/static/js/3rdParty/canvasloader.min.js",type="text/javascript")
	script(src="/static/js/usishi.js",type="text/javascript") 
	script(src="/static/js/buluthan.js",type="text/javascript") 
	script(src="/static/js/guifunctions.js",type="text/javascript")
	//endjsdebug
	block scripts
	script(type="text/javascript")
		loadLogs();
		loadJobs();

		
